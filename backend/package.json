{
  "name": "backend",
  "version": "0.94.0",
  "description": "L!SA middleware",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit",
    "start": "if [ -r ./.env-iam ]; then . ./.env-iam; fi && DOTENV_CONFIG_PATH=.env tsx ./src/server.ts",
    "dev": "if [ -r ./.env-iam ]; then . ./.env-iam; fi && DOTENV_CONFIG_PATH=.env cross-env NODE_ENV=development npx tsx watch --clear-screen=false ./src/server.ts",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 3",
    "lint:fix": "eslint --fix .",
    "test": "jest --passWithNoTests",
    "bundle": "npm run build && npx esbuild src/server.ts --platform=node --target=es2022,node20 --format=esm --inject:./src/cjs-shim.ts --bundle --minify --sourcemap --sources-content=false --outfile=dist/server.mjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.940.0",
    "@aws-sdk/client-s3": "3.940.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.1",
    "@rdfjs/data-model": "2.1.1",
    "@rdfjs/namespace": "2.0.1",
    "@smithy/types": "4.9.0",
    "body-parser": "2.2.1",
    "cookie-parser": "1.4.7",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "envalid": "8.1.1",
    "express": "5.2.1",
    "get-tsconfig": "4.13.0",
    "helmet": "8.1.0",
    "multer": "2.0.2",
    "rdf-sparql-builder": "0.2.5",
    "rdflib": "2.3.0",
    "runtypes": "7.0.4",
    "typescript-eslint": "8.48.0",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.0",
    "@types/cookie-parser": "1.4.10",
    "@types/express": "5.0.6",
    "@types/multer": "2.0.0",
    "@types/node": "24.10.1",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "8.48.0",
    "@typescript-eslint/parser": "8.48.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.3",
    "tsx": "4.21.0",
    "typescript": "~5.9.3"
  },
  "lint-staged": {
    "./src/**/*.{js,ts}": "npm run lint"
  }
}
