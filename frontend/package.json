{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "bundle": "npm run generate-pwa-assets && tsc --noEmit && vite build --outDir ../backend/dist/frontend --emptyOutDir",
    "dev:backend": "vite build --mode development --outDir ../backend/dist/frontend --emptyOutDir --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 3",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/pwa-source.svg",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@lexical/react": "^0.14.2",
    "@lexical/utils": "0.16.1",
    "@tanstack/query-sync-storage-persister": "^5.45.0",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-persist-client": "^5.45.1",
    "date-fns": "^3.4.0",
    "html-react-parser": "^5.1.9",
    "konva": "^9.3.14",
    "lexical": "^0.14.2",
    "maplibre-gl": "^4.5.0",
    "react": "^19.0.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^19.0.0",
    "react-konva": "^19.0.2",
    "react-map-gl": "^7.1.7",
    "react-media-recorder": "^1.7.1",
    "react-router-dom": "^7.2.0",
    "react-select": "^5.8.0",
    "uuid": "^11.0.5",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.66.9",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.3",
    "@types/react-mic": "^12.4.6",
    "@types/react-slick": "^0.23.10",
    "@types/workbox-window": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-testing-library": "^7.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.1.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "workbox-window": "^7.1.0"
  },
  "lint-staged": {
    "./src/**/*.{js,ts}": "npm run lint"
  }
}
