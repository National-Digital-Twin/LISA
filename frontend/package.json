    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/pwa-source.svg",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
