{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --host",
    "dev:ssl": "SSL_ENABLED=true vite --host",
    "build": "tsc && vite build",
    "bundle": "npm run generate-pwa-assets && tsc --noEmit && vite build --emptyOutDir",
